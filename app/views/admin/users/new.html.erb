<div class='index_container'>
  <center>
  <%= form_for @user, url: admin_users_path, method: :post do |form| %>
    <p>First name: <%= form.text_field :first_name %></p>
    <p>Last name: <%= form.text_field :last_name %></p>
    <p>Email: <%= form.text_field :email %></p>
    <p>Password: <%= form.text_field :password %></p>
    <p>Password confirm: <%= form.text_field :password_confirmation %></p>
    <%= form.submit %>
  <% end %>
  </center>
</div>
